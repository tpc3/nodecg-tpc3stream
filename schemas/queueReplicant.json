{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "title": {
            "description": "This is the title",
            "type": "string"
        },
        "author": {
            "description": "Author of the event",
            "type": "string"
        },
        "time":{
            "description": "0:00 ~ 23:59",
            "type": "string"
        },
        "category":{
            "type": "string",
            "examples": [
                "GM",
                "GS",
                "PT",
                "LC",
                "WS",
                "SP",
                "WE"
            ]
        }
	},
	"additionalProperties": false,
	"required": ["title", "author", "time", "category"]
}